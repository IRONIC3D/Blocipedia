<div class="container">
   <h1><%= markdown @wiki.title %></h1> 

   <p><%= markdown @wiki.body %></p>

    <p>
      <b>Name:</b>
      <%= @wiki.original_user.full_name %>
      </p>

      <b>Collaborator:</b>
      <%= @wiki.users.map { |u| u.full_name}.join(", ") %>
      </p>

  <%= link_to 'Edit', edit_wiki_path(@wiki) %> |
  <%= link_to 'Back', wikis_path %>
</div>